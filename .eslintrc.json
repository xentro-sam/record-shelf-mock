{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "google"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "max-len": "off",
        "require-jsdoc": "off",
        "react/jsx-filename-extension": [1, {
        "extensions": [".js", ".jsx"]}
    ]}
}
